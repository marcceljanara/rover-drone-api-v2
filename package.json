{
  "name": "rover-drone-api",
  "version": "1.0.0",
  "description": "Backend API to support Rover Drone project",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "start:dev": "nodemon src/app.js",
    "test": "cross-env NODE_ENV=test jest --setupFiles dotenv/config -i --runInBand",
    "test:export": "cross-env NODE_ENV=test jest --setupFiles dotenv/config -i --runInBand --json --outputFile=test-results.json",
    "test:watch:change": "cross-env NODE_ENV=test jest --watch --setupFiles dotenv/config -i",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll --coverage --setupFiles dotenv/config -i",
    "test:debug": "cross-env NODE_ENV=test jest --setupFiles dotenv/config -i --runInBand --detectOpenHandles",
    "migrate": "node-pg-migrate",
    "migrate:test": "node-pg-migrate -f config/database/test.json",
    "lint": "eslint ./src",
    "lint-fix": "eslint --fix ./src",
    "generate-admin": "node src/utils/generateAdmin.js",
    "clean-table": "node src/utils/cleanTable.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcceljanara/rover-drone-api.git"
  },
  "keywords": [
    "rover",
    "drone",
    "backend",
    "mqtt",
    "rental",
    "iot"
  ],
  "author": "I Nengah Marccel JBC",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marcceljanara/rover-drone-api/issues"
  },
  "homepage": "https://github.com/marcceljanara/rover-drone-api#readme",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "node-pg-migrate": "^7.8.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "@types/jest": "^29.5.14",
    "@vitalets/google-translate-api": "^9.2.1",
    "amqplib": "^0.10.5",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "google-auth-library": "^10.2.1",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mqtt": "^5.10.3",
    "multer": "^2.0.1",
    "nanoid": "^5.0.9",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.16",
    "openai": "^5.12.2",
    "pdfkit": "^0.16.0",
    "pg": "^8.13.1",
    "readable-stream": "^4.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
